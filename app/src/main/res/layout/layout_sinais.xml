<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent"
    app:backgroundTint="@android:color/white"
    android:id="@+id/laySinais">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtSPac"
        android:textStyle="normal|bold"
        android:textColor="@color/azulPadrao"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="38dp"
        android:hint="Nome do paciente"
        android:gravity="center_horizontal"
        android:textSize="20sp" />

    <TextView
        android:text="SO2(%)"
        android:layout_height="16dp"
        android:id="@+id/textView10"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="90dp"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_toRightOf="@+id/textView9"
        android:layout_toEndOf="@+id/textView9"
        android:layout_marginLeft="10dp" />

    <TextView
        android:text="HGT(mg%)"
        android:layout_height="16dp"
        android:id="@+id/textView11"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="90dp"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_toRightOf="@+id/textView10"
        android:layout_toEndOf="@+id/textView10"
        android:layout_marginLeft="10dp" />

    <TextView
        android:text="FR(ipm)"
        android:layout_height="16dp"
        android:id="@+id/textView6"
        android:layout_width="140dp"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignRight="@+id/textView11"
        android:layout_alignEnd="@+id/textView11" />

    <TextView
    android:text="FC(bpm)"
    android:layout_height="16dp"
        android:id="@+id/textView5"
        android:layout_marginLeft="46dp"
    android:layout_width="140dp"
        android:layout_marginStart="46dp"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_below="@+id/etFC"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/etTemp"
        android:textAlignment="gravity"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_width="90dp"
        android:gravity="center_horizontal"
        android:inputType="numberDecimal" />

    <TextView
        android:text="Temp(ºC)"
        android:layout_height="16dp"
        android:id="@+id/textView9"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="90dp"
        android:layout_below="@+id/etTemp"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5" />

    <TextView
        android:text="PAS(mmHg)"
        android:layout_height="16dp"
        android:id="@+id/textView18"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="90dp"
        android:layout_below="@+id/etPAS"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

    <TextView
        android:text="PAM(mmHg)"
        android:layout_height="16dp"
        android:id="@+id/textView21"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="90dp"
        android:layout_below="@+id/txtPAM"
        android:layout_alignLeft="@+id/txtPAM"
        android:layout_alignStart="@+id/txtPAM" />

    <TextView
        android:text="PAD(mmHg)"
        android:layout_height="16dp"
        android:id="@+id/etSFC"
        android:gravity="center_horizontal"
        android:background="@color/verdePadrao"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="90dp"
        android:layout_below="@+id/etPAD"
        android:layout_alignLeft="@+id/etPAD"
        android:layout_alignStart="@+id/etPAD" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:layout_marginTop="50dp"
        android:id="@+id/etFC"
        android:textAlignment="gravity"
        android:layout_width="90dp"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_marginLeft="25dp"
        android:gravity="center_horizontal"
        android:layout_below="@+id/txtSDataHora"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginStart="25dp" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etFR"
        android:textAlignment="gravity"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_width="90dp"
        android:layout_marginLeft="25dp"
        android:gravity="center_horizontal"
        android:layout_marginStart="25dp"
        android:layout_above="@+id/textView6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etPAD"
        android:textAlignment="gravity"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_width="90dp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_above="@+id/textView21"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etPAS"
        android:textAlignment="gravity"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_marginTop="50dp"
        android:layout_width="90dp"
        android:gravity="center_horizontal"
        android:layout_below="@+id/textView9"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etSO2"
        android:textAlignment="gravity"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:layout_width="90dp"
        android:gravity="center_horizontal"
        android:layout_above="@+id/textView10"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:textStyle="normal|bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtSDataHora"
        android:textColor="@color/verdePadrao"
        android:textSize="12sp"
        android:hint="Data e hora da evolução"
        android:gravity="center_horizontal|center"
        android:textAlignment="gravity"
        android:layout_below="@+id/txtSPac"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="0"
        android:layout_height="wrap_content"
        android:id="@+id/txtPAM"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="45sp"
        android:textStyle="bold"
        android:gravity="center_horizontal"
        android:layout_width="90dp"
        android:layout_above="@+id/textView18"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etHGT"
        android:textAlignment="gravity"
        android:textColor="@color/azulPadrao"
        android:textSize="30sp"
        android:layout_width="90dp"
        android:textStyle="normal|bold"
        android:gravity="center_vertical|center_horizontal"
        android:layout_above="@+id/textView11"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

</RelativeLayout>